generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model EmojiList {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  emoji   String   @unique
  answerTR    String 
  answerEN String 
  answerSP String 
  answerDU String 
  startTime DateTime?
  endTime DateTime?
}